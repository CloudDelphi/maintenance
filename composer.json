{
    "name": "mbonaldo/maintenance",
    "description": "Handle Maintenance page",
    "license": "MIT",
    "authors": [
        {
            "name": "Mauro Bonaldo",
            "email": "mbonaldo@gmail.com",
            "homepage": "http://www.bonaldo.cloud/"
        }
    ],
    "homepage": "https://github.com/CloudDelphi/maintenance",
    "keywords": ["Laravel", "Maintenance"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MBonaldo\\Maintenance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MBonaldo\\Maintenance\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MBonaldo\\Maintenance\\MaintenanceServiceProvider"
            ],
            "aliases": {
                "Maintenance": "MBonaldo\\Maintenance\\Facades\\Maintenance"
            }
        }
    }
}
